q_install=y
q_puppet_cloud_install=y
q_puppet_enterpriseconsole_auth_password=cfn_password
q_puppet_enterpriseconsole_auth_user=cfn_user
q_puppet_enterpriseconsole_database_install=y
q_puppet_enterpriseconsole_database_name=console
q_puppet_enterpriseconsole_database_password=anotherstrongpassword
q_puppet_enterpriseconsole_database_root_password=astrongpassword
q_puppet_enterpriseconsole_database_user=console
q_puppet_enterpriseconsole_httpd_port=443
q_puppet_enterpriseconsole_install=y
q_puppet_enterpriseconsole_inventory_hostname=localhost.localdomain
q_puppet_enterpriseconsole_inventory_port=8140
q_puppet_enterpriseconsole_master_hostname=localhost.localdomain
q_puppet_symlinks_install=y
q_puppetagent_certname=localhost.localdomain
q_puppetagent_install=y
q_puppetagent_server=`/opt/aws/bin/ec2-metadata --local-hostname | awk --compat -F': ' '{print $2}'`
q_puppetmaster_certname=`/opt/aws/bin/ec2-metadata --local-hostname | awk --compat -F': ' '{print $2}'`
q_puppetmaster_dnsaltnames=`/opt/aws/bin/ec2-metadata --public-hostname | awk --compat -F': ' '{print $2}'`,`/opt/aws/bin/ec2-metadata --local-hostname | awk --compat -F': ' '{print $2}'`
q_puppetmaster_enterpriseconsole_hostname=localhost
q_puppetmaster_enterpriseconsole_port=443
q_puppetmaster_forward_facts=n
q_puppetmaster_install=y
q_vendor_packages_install=y
